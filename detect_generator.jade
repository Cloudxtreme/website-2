h1.cpxTitle Detect Command Generator Beta
h3 By TSLNC04
h6 1.8+ (Will work in snapshots)

br
br
br

div.container
  div.row
    div.col-xs-12
      div.border
        h3.cpxTitle Main Options
        p.big Entity Selector: 
          input(name="entitySelector" type="text" id="entitySelector" style="font-size: 0.875em")

        p.big Scoreboard Entity Selector: 
          input(name="scoreEntitySelector" type="text" id="scoreEntitySelector" style="font-size: 0.875em")

        p.big Scoreboard Objective: 
          input(name="detectObjective" type="text" id="detectObjective" style="font-size: 0.875em")
  
        p.big Scoreboard Amount: 
          input(name="detectAmt" type="text" id="detectAmt" style="font-size: 0.875em")
          
        p.big Entity Data (Optional): 
          input(name="entityData" type="text" id="entityData" style="font-size: 0.875em")
        
    div.col-xs-12
      br
      h3.cpxTitle Command
      div.border
        h5#commandOutput Click Generate to see your command!
br
br
br

script(src="js/detectGenerator.js")

h3.cpxTitle Block Information
div.cpx-center
  a(type="button" class="btn btn-danger" onclick="generateDetectCommand();")
    i.fa.fa-arrow-right  Generate
  a(type="button" class="btn btn-danger" onclick="exportGeneration();")
    i.fa.fa-arrow-right  Export Save Code
  a(type="button" class="btn btn-danger" onclick="importGeneration();")
    i.fa.fa-arrow-right  Import Save Code

br
p.big
  textarea(name="blockInformation" id="blockInformation" rows="10" cols="128" style="font-size: 1em")

